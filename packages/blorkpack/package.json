{
  "name": "@littlecarlito/blorkpack",
  "version": "0.12.12",
  "description": "Asset management utilities for 3D scenes",
  "type": "module",
  "blorkType": "package",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./app-renderer": "./dist/app_renderer.js",
    "./asset-activator": "./dist/asset_activator.js",
    "./spawner": "./dist/asset_spawner/asset_spawner.js",
    "./system-asset-types": "./dist/asset_spawner/system_factory/system_asset_types.js",
    "./system-factory": "./dist/asset_spawner/system_factory/system_factory.js",
    "./box-spawner": "./dist/asset_spawner/system_factory/system_spawners/box_spawner.js",
    "./capsule-spawner": "./dist/asset_spawner/system_factory/system_spawners/capsule_spawner.js",
    "./cylinder-spawner": "./dist/asset_spawner/system_factory/system_spawners/cylinder_spawner.js",
    "./sphere-spawner": "./dist/asset_spawner/system_factory/system_spawners/sphere_spawner.js",
    "./spotlight-spawner": "./dist/asset_spawner/system_factory/system_spawners/spotlight_spawner.js",
    "./asset-storage": "./dist/asset_storage.js",
    "./blorkpack-flags": "./dist/blorkpack_flags.js",
    "./custom-type-manager": "./dist/custom_type_manager.js",
    "./loaders": "./dist/loaders.js",
    "./manifest-manager": "./dist/manifest_manager.js",
    "./manifest-types": "./dist/manifest_types.js",
    "./physics-util": "./dist/physics/physics_util.js",
    "./rapier-proxy": "./dist/rapier_proxy.js",
    "./three-proxy": "./dist/three_proxy.js",
    "./asset_spawner/system_factory/system_spawners": "./dist/asset_spawner/system_factory/system_spawners/index.js",
    "./physics": "./dist/physics/index.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build && node scripts/generate-exports.js",
    "dev": "npm run build && vite build --watch",
    "generate-exports": "node scripts/generate-exports.js",
    "test": "node examples/manifest-test.js",
    "example:usage": "node examples/usage-example.js",
    "example:manifest": "node examples/manifest-test.js",
    "example:exports": "node examples/export-test.js",
    "clean": "rimraf dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "three": "^0.172.0",
    "@dimforge/rapier3d-compat": "^0.14.0"
  },
  "devDependencies": {
    "vite": "^6.0.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "semantic-release": "^23.0.2",
    "rimraf": "^5.0.5",
    "eslint": "^8.56.0",
    "license-webpack-plugin": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littlecarlito/threejs_site.git",
    "directory": "packages/blorkpack"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "copyright": "Copyright (C) 2024 Steven Meier"
}
